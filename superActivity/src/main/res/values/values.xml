    <style name="DividerConfirm">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/super_divider_list</item>
    </style>
    <style name="LinerLayoutConfirm">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">57dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="SuperAppTheme" parent="android:Theme.Light.NoTitleBar">
        <!--Window color-->
        <item name="android:windowBackground">@android:color/white</item>
    </style>
    <style name="TVLiveStep">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textColor">@color/super_selector_step</item>
        <item name="android:background">@drawable/super_selector_live_step</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="TextConfirmLeft">
        <item name="android:layout_marginLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#282828</item>
    </style>
    <style name="TextConfirmRight">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/super_real_gray</item>
    </style>
    <style name="TvStep">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/super_selector_step</item>
        <item name="android:textSize">10sp</item>
    </style>
